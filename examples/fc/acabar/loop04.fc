.vars {
  x, y, n
}

.pvars {
  x', y', n'
}

.trans t0 : n -> n {
  -1 * n <= -201,
  1 * y <= 8,
  1 * x + -1 * n <= -1,
  -1 * x + -1 * y + 1 * x' = 0,
  1 * x' <= 199,
  -1 * y + 1 * y' = 0,
  -1 * n + 1 * n' = 0
}
.trans t1 : n -> n {
  -1 * n <= -201,
  1 * y <= 8,
  -1 * x + 1 * n <= 0,
  -1 * x + 1 * x' = 0,
  -1 * y + 1 * y' = 0,
  -1 * n + 1 * n' = 0
}

